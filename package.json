{
  "name": "fastify-best-practices",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build-dev": "npm run build && NODE_ENV=development node --inspect dist/server.js",
    "start:dev": "npm run build && NODE_ENV=production node --watch --env-file=example.env dist/server.js",
    "start": "npm run build && NODE_ENV=production node --env-file=prod.env dist/server.js",
    "lint": "pnpm biome lint src",
    "format": "pnpm biome format src --write",
    "test": "node --test src/**/*",
    "test:ci": "NODE_V8_COVERAGE=native/tests/  node --test --experimental-test-coverage native/tests/ ",
    "prepare": "husky"
  },
  "author": "ald",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/sensible": "^5.5.0",
    "amqplib": "^0.10.4",
    "config-yaml": "^1.1.6",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "joi": "^17.12.3",
    "pg-promise": "^11.6.0",
    "pino-pretty": "^11.0.0",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.12.7",
    "bcrypt": "^5.1.1",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "pino": "^8.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
